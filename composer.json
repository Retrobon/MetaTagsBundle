{
  "name": "retrobon/metatags-bundle",
  "version": "0.1.0",
  "type": "symfony-bundle",
  "description": "Symfony MetaTags bundles",
  "license": "proprietary",
  "require": {
    "php": "^7.4"
  },
  "require-dev": {
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "retrobon\\MetaTagsBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "retrobon\\MetaTagsBundle\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/simple-phpunit"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.0.*"
    }
  }
}
